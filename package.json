{
  "name": "@cycjimmy/config-lib",
  "version": "0.0.0-development",
  "type": "module",
  "description": "Personal collection of configs",
  "scripts": {
    "lint": "eslint -c .eslintrc .",
    "lint:fix": "eslint --fix -c .eslintrc .",
    "babel": "npx babel src --out-dir dist/esm",
    "handleExtensionCJS": "node ./tasks/handleExtensionCJS.js",
    "transform2cjs": "cross-env NODE_ENV=cjs npx babel dist/esm --out-dir dist/cjs --out-file-extension .cjs && npm run handleExtensionCJS",
    "build": "npm run lint:fix && trash dist && npm run babel && npm run transform2cjs",
    "package": "npm run build && node ./tasks/modifyPackage.js && gulp copy:package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cycjimmy/config-lib.git"
  },
  "keywords": [
    "collection",
    "config"
  ],
  "author": "cycjimmy <cycjimmy@gmail.com> (https://github.com/cycjimmy)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cycjimmy/config-lib/issues"
  },
  "homepage": "https://github.com/cycjimmy/config-lib#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.7",
    "@babel/preset-env": "^7.16.11",
    "cross-env": "^7.0.3",
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "gulp": "^4.0.2",
    "trash-cli": "^5.0.0"
  }
}
